<% content_for(:heading, "Login") %>
<%= form_tag sessions_path, id: "login_user"  do %>
<%= label_tag :username %>
<%= text_field_tag :username, nil, autocomplete: :off %>
<br />
<%= label_tag :password %>
<%= password_field_tag :password %>
<br />
<br />
<%= submit_tag "Sign in", class: "c-btn" %>

<% end %>
